  <title>Codeforces tracker</title>
  <style media="screen">
    nav {
      box-shadow: rgba(0, 0, 0, 0.19) 0px 10px 20px, rgba(0, 0, 0, 0.23) 0px 6px 6px;
      background-color: var(--primary);
    }
  </style>
</head>

<body>

  <%- include('./partials/nav.ejs') %>

  <!-- problem sets container goes from here -->

  <div class="container" id="problem-box">
    

    <div class="card problem-card">
      <div class="card-header">
        Solved Problems
      </div>
     
      <div class="card-body">
        <ol>
          <% var current = data2.solvedQuestion;
          var i = 0; %>
            <% for(var i=0; i < current.length; i++) { %>
            <li class="solved"><a href=""><%= data2.solvedQuestion[i].problem.name %></a></li>
             <% } %>

        </ol>
      </div>
    </div>

    <!-- <div class="card problem-card">
